@model PresentationService.Models.AdminModels.ConsignmentModels.Items.ConsignmentIndexItemModel
@{
    Layout = null;
}
<tr class="@(ViewBag.isOddRow ? "odd" : "even")" onclick="javascript:location.href = '@Url.Action("Edit", "Consignment", new { id = Model.ConsignmentId })';">
    <td>
        @Model.DateLoad
    </td>
    <td>
        @Model.UserEmail
    </td>
    <td>
        @Model.CountDifferentProducts
    </td>
    <td>
        @Model.CountProducts
    </td>
</tr>